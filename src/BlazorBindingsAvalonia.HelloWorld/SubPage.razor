@page "/sub"
@inject INavigation navigation

@using Avalonia.Media


<ScrollViewer Background="Avalonia.Media.Colors.Red">
    <StackPanel HorizontalAlignment="HorizontalAlignment.Center" VerticalAlignment="VerticalAlignment.Top">
        <TextBlock Text="Sub" Foreground="Colors.White"></TextBlock>
        <Button OnClick="Back">Back</Button>
    </StackPanel>
</ScrollViewer>

@code {
    public async Task Back()
    {
        await navigation.PopAsync();
    }
}